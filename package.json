{
  "name": "mario5",
  "version": "2.0.0",
  "description": "TypeScript version of the Mario5 demo application.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node fuse",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "yarn lint",
    "prettier": "prettier --config prettier.config.js --write src/**/*.{ts,tsx}",
    "build": "rimraf dist && cross-env NODE_ENV=production node fuse dist"
  },
  "author": "Florian Rappl",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.1.4",
    "fuse-box": "^3.2.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.3.22"
  }
}
